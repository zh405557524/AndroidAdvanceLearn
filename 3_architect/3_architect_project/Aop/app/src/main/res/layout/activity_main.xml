<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/bt_InvocationHandler"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="动态代理"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bt_aspectj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="aspectj 切面"
        app:layout_constraintLeft_toLeftOf="@id/bt_InvocationHandler"
        app:layout_constraintRight_toRightOf="@id/bt_InvocationHandler"
        app:layout_constraintTop_toBottomOf="@id/bt_InvocationHandler" />

</androidx.constraintlayout.widget.ConstraintLayout>